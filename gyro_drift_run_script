/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_sigma_plot.perl gyro_drift_hist_roll LETG RETR

/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_sigma_plot.perl gyro_drift_hist_pitch HETG INSR
/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_sigma_plot.perl gyro_drift_hist_pitch HETG RETR
/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_sigma_plot.perl gyro_drift_hist_pitch LETG INSR
/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_sigma_plot.perl gyro_drift_hist_pitch LETG RETR

/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_sigma_plot.perl gyro_drift_hist_yaw HETG INSR
/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_sigma_plot.perl gyro_drift_hist_yaw HETG RETR
/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_sigma_plot.perl gyro_drift_hist_yaw LETG INSR
/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_sigma_plot.perl gyro_drift_hist_yaw LETG RETR

/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_prep_data_html.perl

/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_print_gyro_html.perl

/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_create_fig_html.perl HETG INSR
