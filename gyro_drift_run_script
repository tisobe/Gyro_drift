source /home/mta/.ascrc

rm -rf param
mkdir param
source /home/mta/bin/reset_param
setenv PFILES "${PDIRS}:${SYSPFILES}"
set path = (/home/ascds/DS.release/bin/  $path)

/opt/local/bin/perl /data/mta/MTA/bin/gyro_drift_detail.perl

